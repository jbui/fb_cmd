<div class="row" style="text-align: center;">
	<img src="assets/macbook.png"/><br/>
	<div id="container_buttons" style="margin-top: -30px;">
        <p><a class="a_demo_four" href="https://github.com/lewisf/fb_cmd_chrome/raw/master/fb_cmd_chrome.crx" >
            Install the Chrome Extension
        </a></p>
    </div>
    <div class="description">
    	Download the Chrome Extension to get started
    </div>

    <% if @jobs != nil then 
        i =  1
    %>

    <div class="cron">
        <table class="table">
            <thead>
              <tr>
                <th>#</th>
                <th>Cron Jobs</th>
              </tr>
            </thead>
            <tbody>

            <% @jobs.each do |job| %>
               <tr>
                <td><%= i %> </td>
                <td><%=job['command']%></td>
              </tr>
              <% i = i + 1 %>
            <% end %>

            </tbody>
          </table>
    </div>

    <% end %>

</div>